{
  "family": "ms-task-def",
  "executionRoleArn": "arn:aws:iam::414207599053:role/ecsTaskExecution",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ms-container",
      "image": "414207599053.dkr.ecr.ap-northeast-2.amazonaws.com/showmethatcode:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}